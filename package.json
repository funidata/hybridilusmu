{
  "name": "hybridilusmu",
  "version": "1.0.0",
  "description": "Toteutettu Helsingin yliopiston ohjelmistotuotantoprojektikurssin työnä syksyllä 2021.",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": "./dev/.eslintrc.js"
  },
  "scripts": {
    "init": "npx sequelize-cli db:create && npm run migrate",
    "start": "npm run migrate && node ./src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "test": "NODE_ENV=unittest nyc --reporter=html mocha",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "setup-e2e": "pip3 install --upgrade --target ./test/e2e/testcases/site-packages/ --requirement ./test/e2e/requirements.txt",
    "eslint": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funidata/hybridilusmu.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/funidata/hybridilusmu/issues"
  },
  "homepage": "https://github.com/funidata/hybridilusmu#readme",
  "dependencies": {
    "@slack/bolt": "^3.6.0",
    "dotenv": "^10.0.0",
    "espree": "^9.0.0",
    "luxon": "^2.5.2",
    "node-schedule": "^2.0.0",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "codecov": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0"
  }
}
